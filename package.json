{
  "name": "cratematch-web",
  "version": "1.0.0",
  "description": "Web version of CrateMatch - Spotify playlist to Serato crate converter",
  "main": "server.js",
  "engines": {
    "node": "18.x",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "preinstall": "npm config set registry https://registry.npmjs.org/",
    "postinstall": "npm cache clean --force",
    "deploy-setup": "./scripts/deploy-setup.sh"
  },
  "dependencies": {
    "@musiclibrarytools/mlt.js": "^5.1.13",
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7"
  },
  "overrides": {
    "@musiclibrarytools/wav-tag-node": "npm:mp3tag.js@^3.11.4",
    "@musiclibrarytools/mp4-tag-node": "npm:mp3tag.js@^3.11.4",
    "@musiclibrarytools/serato.js": "npm:mp3tag.js@^3.11.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "serato",
    "spotify",
    "playlist",
    "crate",
    "music"
  ],
  "author": "CrateMatch Team",
  "license": "MIT"
}
