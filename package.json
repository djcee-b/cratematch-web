{
  "name": "cratematch-web",
  "version": "1.0.0",
  "description": "Web version of CrateMatch - Spotify playlist to Serato crate converter",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "bash scripts/build.sh",
    "setup": "bash scripts/setup-public.sh",
    "postinstall": "bash scripts/copy-public.sh",
    "load-test": "node load-test.js",
    "load-test-simple": "node load-test-simple.js",
    "load-test-playlist": "node playlist-load-test.js",
    "load-test-apache": "bash load-test-apache-bench.sh",
    "load-test-wrk": "bash load-test-wrk.sh",
    "load-test-all": "npm run load-test-simple && npm run load-test-apache && npm run load-test-wrk"
  },
  "dependencies": {
    "@musiclibrarytools/mlt.js": "^v5.1.20",
    "@musiclibrarytools/serato.js": "^1.2.1",
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  },
  "keywords": [
    "serato",
    "spotify",
    "playlist",
    "crate",
    "music"
  ],
  "author": "CrateMatch Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  }
}
